  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "bundle:all": "yarn run bundle:chrome; yarn run bundle:safari; yarn run bundle:edge; yarn run bundle:firefox; yarn run bundle:fallback; yarn run bundle:server",
    "bundle:chrome": "BABEL_ENV=build.chrome node_modules/.bin/webpack --config config/webpack.browsers.js --json > dist/chrome/webpack.json",
    "bundle:safari": "BABEL_ENV=build.safari node_modules/.bin/webpack --config config/webpack.browsers.js --json > dist/safari/webpack.json",
    "bundle:edge": "BABEL_ENV=build.edge node_modules/.bin/webpack --config config/webpack.browsers.js --json > dist/edge/webpack.json",
    "bundle:firefox": "BABEL_ENV=build.firefox node_modules/.bin/webpack --config config/webpack.browsers.js --json > dist/firefox/webpack.json",
    "bundle:fallback": "BABEL_ENV=build.fallback node_modules/.bin/webpack --config config/webpack.browsers.js --json > dist/fallback/webpack.json",
    "bundle:server": "BABEL_ENV=build.server node_modules/.bin/webpack --config config/webpack.server.js --json > dist/server/webpack.json",
    "bundle:prod": "cross-env NODE_ENV=production yarn run bundle:all;",
    "wp-server": "BABEL_ENV=build.chrome NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "dev-server": "cross-env NODE_ENV=development node dist/server/server.js",
    "start": "yarn run dev-server | yarn run wp-server"
  },
